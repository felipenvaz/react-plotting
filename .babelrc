{
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ],
    "compact": false,
    "env": {
        "cjs": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "browsers": [
                                "last 3 versions"
                            ]
                        }
                    }
                ],
                "@babel/preset-react",
                "@babel/preset-typescript"
            ]
        },
        "esm": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "browsers": [
                                "last 3 versions"
                            ]
                        },
                        "modules": false
                    }
                ],
                "@babel/preset-react",
                "@babel/preset-typescript"
            ]
        }
    }
}
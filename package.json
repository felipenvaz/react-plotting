{
    "name": "react-plotting",
    "version": "0.1.16",
    "description": "",
    "main": "./lib/cjs/index.js",
    "module": "./lib/esm/index.js",
    "types": "./lib/esm/index.d.ts",
    "scripts": {
        "build": "babel ./src --extensions \".ts,.tsx\" --source-maps",
        "build:cjs": "cross-env BABEL_ENV=cjs npm run build -- --out-dir lib/cjs",
        "build:esm": "cross-env BABEL_ENV=esm npm run build -- --out-dir lib/esm",
        "prepublishOnly": "npm run generatePackage",
        "generatePackage": "npm run lint && npm run declarationFiles && npm run build:cjs && npm run build:esm",
        "typescript:watch": "tsc",
        "declarationFiles": "tsc --isolatedModules false --jsx \"react\" --emitDeclarationOnly --declaration --declarationDir lib/esm --lib es5,es6,dom src/index.ts",
        "lint": "tslint 'src/**/*'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/felipenvaz/react-plotting.git"
    },
    "author": "felipenvaz",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/felipenvaz/react-plotting/issues"
    },
    "homepage": "https://github.com/felipenvaz/react-plotting#readme",
    "peerDependencies": {
        "react": "^16.8.2",
        "react-dom": "^16.8.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.3",
        "@babel/plugin-proposal-class-properties": "^7.3.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.3.3",
        "@types/react": "^16.3.13",
        "@types/react-dom": "^16.0.5",
        "@types/react-measure": "^2.0.2",
        "cross-env": "^5.2.0",
        "react": "^16.8.2",
        "react-dom": "^16.8.2",
        "react-measure": "^2.2.4",
        "tslint": "^5.12.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-react": "^3.6.0",
        "typescript": "^3.3.3"
    }
}